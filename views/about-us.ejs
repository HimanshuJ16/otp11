<script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>

<script>
  $(function () {
    var fx = function fx() {
        $(".stat-number").each(function (i, el) {
            var data = parseFloat($(el).data("n")); // Read the initial value from the HTML
            var initialCount = 0; // Set initial count for all elements

            var props = {
                "from": {
                    "count": initialCount
                },
                "to": {
                    "count": data
                }
            };

            $(props.from).animate(props.to, {
                duration: 1000 * 2, // Change this value to control the speed (2 seconds in this example)
                step: function (now, fx) {
                    // Use Indian numbering system (thousands separator) for formatting
                    $(el).text(Math.ceil(now).toLocaleString('en-IN') + "+");
                },
                complete: function () {
                    if (el.dataset.sym !== undefined) {
                        el.textContent = el.textContent.concat(el.dataset.sym);
                    }
                }
            });
        });
    };

    var reset = function reset() {
        if ($(window).scrollTop() > 90) {
            $(window).off("scroll", reset);
            fx();
        }
    };

    $(window).on("scroll", reset);
});

</script>


