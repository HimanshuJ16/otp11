<script>
    function submitForm() {
        // Get form data
        var name = document.getElementById('name').value;
        var phone = document.getElementById('phone').value;
        var subjectDropdown = document.getElementById('subject');
        var subject = subjectDropdown.options[subjectDropdown.selectedIndex].text;
        var message = document.getElementById('message').value;

        // Check if all fields are filled
        if (!name || !phone || subject === "Select Subject*" || !message) {
            alert('Please fill all the fields');
            return;
        }

        // Create email template
        var emailBody = "While navigating through your website, I made the decision to fill out the inquiry form as part of my engagement with your online platform.\n\n";
        emailBody += "Full Name: " + name + "\n";
        emailBody += "Phone Number: " + phone + "\n";
        emailBody += "Subject: " + subject + "\n";
        emailBody += "Message: " + message + "\n\n";
        emailBody += "I'm interested in your services and am eager to connect soon. Thank you!";

        // Create mailto link
        var mailtoLink = "mailto:contact@hsjnfinwisdom.com?subject=" + encodeURIComponent(subject) + "&body=" + encodeURIComponent(emailBody);

        // Open default email client
        window.location.href = mailtoLink;
    }
</script>

