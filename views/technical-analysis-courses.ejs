<script>
  function submitForm() {
    var form = document.getElementById("myForm");
    var formData = new FormData(form);

    // Validate required fields
    var requiredFields = ['Name', 'Occupation', 'Inquiry', 'PhoneNumber'];
    for (var i = 0; i < requiredFields.length; i++) {
    var fieldName = requiredFields[i];
    if (!formData.get(fieldName)) {
        alert("Please fill all the fields");
        return; // Stop form submission if a required field is empty
      }
    }

    var prewrittenText1 = document.getElementById("prewrittenText1").value;
    var prewrittenText2 = document.getElementById("prewrittenText2").value;

    var emailBody = prewrittenText1 + "\n\n";

    formData.forEach(function(value, key){
      emailBody += key + ": " + value + "\n";
    });

    emailBody += "\n" + prewrittenText2;
    var emailSubject = "Trading Courses Inquiry";

    var mailtoLink = "mailto:hsjnfinwisdom@gmail.com?subject=" + encodeURIComponent(emailSubject) + "&body=" + encodeURIComponent(emailBody);

    window.location.href = mailtoLink;
  }
</script>
